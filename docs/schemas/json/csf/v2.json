{
  "$ref": "#/definitions/Shimakaze.Sdk.Csf.V2.CsfFile",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Shimakaze.Sdk.Csf.V1.CsfAdvancedValue": {
      "additionalProperties": false,
      "description": "Csf复杂值",
      "properties": {
        "extra": {
          "type": "string"
        },
        "value": {
          "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfSimpleValue"
        }
      },
      "required": [
        "value"
      ],
      "type": "object"
    },
    "Shimakaze.Sdk.Csf.V1.CsfLanguage": {
      "description": "Csf支持的语言",
      "enum": [
        "en_US",
        "en_UK",
        "de",
        "fr",
        "es",
        "it",
        "jp",
        "Jabberwockie",
        "kr",
        "zh"
      ],
      "type": "string"
    },
    "Shimakaze.Sdk.Csf.V1.CsfSimpleValue": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      ],
      "description": "Csf简单值"
    },
    "Shimakaze.Sdk.Csf.V1.CsfValue": {
      "anyOf": [
        {
          "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfSimpleValue"
        },
        {
          "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfAdvancedValue"
        }
      ],
      "description": "Csf值"
    },
    "Shimakaze.Sdk.Csf.V2.CsfDataValue": {
      "anyOf": [
        {
          "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfSimpleValue"
        },
        {
          "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfAdvancedValue"
        },
        {
          "$ref": "#/definitions/Shimakaze.Sdk.Csf.V2.CsfMultiValue"
        },
        {
          "type": "null"
        }
      ],
      "description": "Csf标签的字符串值部分"
    },
    "Shimakaze.Sdk.Csf.V2.CsfFile": {
      "additionalProperties": false,
      "description": "Csf文件",
      "properties": {
        "data": {
          "additionalProperties": {
            "$ref": "#/definitions/Shimakaze.Sdk.Csf.V2.CsfDataValue"
          },
          "type": "object"
        },
        "language": {
          "anyOf": [
            {
              "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfLanguage"
            },
            {
              "type": "number"
            }
          ]
        },
        "protocol": {
          "const": 2,
          "type": "number"
        },
        "version": {
          "enum": [
            2,
            3
          ],
          "type": "number"
        }
      },
      "required": [
        "protocol",
        "data"
      ],
      "type": "object"
    },
    "Shimakaze.Sdk.Csf.V2.CsfMultiValue": {
      "additionalProperties": false,
      "description": "Csf多值",
      "properties": {
        "values": {
          "items": {
            "$ref": "#/definitions/Shimakaze.Sdk.Csf.V1.CsfValue"
          },
          "type": "array"
        }
      },
      "required": [
        "values"
      ],
      "type": "object"
    }
  }
}