{
  "name": "shimakaze-sdk-vscode",
  "version": "0.0.0",
  "displayName": "Shimakaze.Sdk for VSCode",
  "description": "适用于 VSCode 的 Shimakaze.Sdk 工具集",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShimakazeProject/Shimakaze.Sdk.VSCode.git"
  },
  "author": {
    "name": "frg2089",
    "email": "frg2089@outlook.com",
    "url": "https://github.com/frg2089"
  },
  "publisher": "frg2089",
  "packageManager": "yarn@4.1.0",
  "engines": {
    "node": ">=18",
    "vscode": "^1.85.0"
  },
  "main": "dist/extension/index.js",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "lint": "eslint '{src,extension}/**'",
    "packup": "vsce pack --no-dependencies",
    "preview": "vite preview",
    "vscode:prepublish": "yarn build"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@tomjs/vite-plugin-vscode": "^1.5.0",
    "@tomjs/vscode-extension-webview": "^1.2.0",
    "@types/eslint": "^8",
    "@types/node": "^18.19.14",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vscode/vsce": "^2.23.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-pkg": "^0.18.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "Shimakaze.Sdk",
        "properties": {
          "shimakaze.sdk.languageClient.LogLevel": {
            "type": "integer",
            "description": "语言服务器的日志等级。",
            "default": 1,
            "enum": [
              0,
              1,
              2,
              3
            ],
            "enumItemLabels": [
              "禁用",
              "消息",
              "简略",
              "详细"
            ],
            "enumDescriptions": [
              "完全禁用日志",
              "仅记录消息",
              "不那么详细的日志",
              "详细的日志"
            ]
          }
        }
      }
    ]
  },
  "extensionDependencies": []
}
