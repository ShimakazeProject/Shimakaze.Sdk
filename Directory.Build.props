<Project>
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup Condition="'$(MSBuildProjectFullPath)' != '$(MSBuildThisFileDirectory)src\Shimakaze.Sdk.Utils\Shimakaze.Sdk.Utils.csproj'">
    <ProjectReference Include="$(MSBuildThisFileDirectory)src\Shimakaze.Sdk.Utils\Shimakaze.Sdk.Utils.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Using Include="Shimakaze.Sdk.Utils"/>
  </ItemGroup>

  <ItemGroup>
    <None Update="Assets\*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
