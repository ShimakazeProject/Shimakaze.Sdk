{
  "compilerOptions": {
    "composite": true,
    "rootDir": "./extension",
    "baseUrl": "./extension",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@shimakaze.sdk/extension/*": ["./*"],
      "@shimakaze.sdk/extension": ["./index.ts"]
    }
  },
  "include": ["./extension/**/*.ts", "./extension/**/*.d.ts"]
}
